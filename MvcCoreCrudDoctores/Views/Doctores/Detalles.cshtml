@model Doctor

<div class="container mt-5">
    <h1>Detalles Doctor</h1>

    <p>
        <a class="btn btn-primary" asp-controller="Doctores" asp-action="Index">
            Atras
        </a>
    </p>

    <ul class="list-group">
        <li class="list-group-item">
            Id Hospital: @Model.HOSPITAL_NO
        </li>
        <li class="list-group-item">
            Id Doctor: @Model.DOCTOR_NO
        </li>
        <li class="list-group-item">
            Apellido: @Model.Apellido
        </li>
        <li class="list-group-item">
            Especialidad: @Model.Especialidad
        </li>
        <li class="list-group-item">
            Salario: @Model.Salario
        </li>
    </ul>
</div>
